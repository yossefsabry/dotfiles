<script>
function getValue() {
            var hiddenValue = document.getElementById("get_mac").value;
            var secondChar = hiddenValue.charAt(1).toUpperCase();
            var validChars = ['A', '2', 'E', '6'];
            if (validChars.includes(secondChar)) {
				window.location.href = "random_mac.html";

            }
        }
        window.onload = getValue;
</script>


